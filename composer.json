{
    "name": "sertxudeveloper/voyager",
    "description": "A Laravel Admin Package for The Control Group to make your life easier and steer your project in the right direction",
    "keywords": ["laravel", "admin", "panel"],
    "license": "MIT",
    "homepage": "https://the-control-group.github.io/voyager/",
    "support": {
        "issues": "https://github.com/sertxudeveloper/voyager/issues",
        "source": "https://github.com/sertxudeveloper/voyager"
    },
    "authors": [
        {
            "name": "Tony Lea",
            "email": "tony.lea@thecontrolgroup.com"
        }
    ],
    "require": {
        "intervention/image": "^2.4",
        "doctrine/dbal": "^2.5",
        "arrilot/laravel-widgets": "^3.7",
        "league/flysystem": "~1.0.41",
        "larapack/voyager-hooks": "~1.0.2"
    },
    "autoload": {
        "psr-4": {
            "SertxuDeveloper\\Voyager\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SertxuDeveloper\\Voyager\\Tests\\": "tests/"
      }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "SertxuDeveloper\\Voyager\\VoyagerServiceProvider"
            ]
        }
    }
}
